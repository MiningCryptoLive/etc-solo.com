
  <div class="row">
    <div class="col-xs-12 col-md-12">
	  <div class="note note-success">
		{{#if model.matured}}
		<h4 style="color:white">Matured Blocks</h4>
		<div class="table-responsive">
		<table class="table table-condensed table-striped">
		  <thead>
			<tr>
			  <th style="color:white">Height</th>
			  <th style="color:white">Block Hash</th>
			  <th style="color:white">Time Found</th>
			  <th style="color:white">Variance</th>
			  <th style="color:white">Reward</th>
			  <th style="color:white">Type</th>
			  <th style="color:white">Shares/Diff</th>
			  <th style="color:white">Worker ID</th>
			</tr>
		  </thead>
		  <tbody>
			{{#each model.matured as |block|}}
			  {{partial "blocks/block"}}
			{{/each}}
		  </tbody>
		</table>
		</div>
		{{else}}
		<h3 style="color:white">No matured blocks!</h3>
		{{/if}}
	  </div>
	</div>
  </div>

